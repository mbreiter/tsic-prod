run_callback_cs:
	cd $(GRBAPP); \
	rm -f *.cs; \
	cp $(KEEPTU)/c#/callback_cs.cs .; \
	dotnet run $(KEEPTU)/data/p0033.mps; \
	cd -
run_dense_cs:
	cd $(GRBAPP); \
	rm -f *.cs; \
	cp $(KEEPTU)/c#/dense_cs.cs .; \
	dotnet run; \
	cd -
